{
    "manifest_version": 3,
    "name": "現在時刻表示",
    "version": "1.0",
    "description": "ブラウザのツールバーに現在時刻を表示します",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": []
  }